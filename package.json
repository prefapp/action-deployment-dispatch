{
  "name": "action-deployment-dispatch",
  "version": "1.0.0",
  "description": "A github action for deployments dispatch to a gitops repo",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "eslint .",
    "prepare": "ncc build index.js -o dist --source-map --license licenses.txt",
    "test": "jest",
