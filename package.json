  },
  "homepage": "https://github.com/prefapp/action-deployment-dispatch#readme",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.0",
    "js-yaml": "^4.1.0",
    "jsonschema": "^1.4.0"
  },
  "devDependencies": {
    "@actions/github": "^6.0.0",
    "@eslint-recommended/eslint-config": "^26.1.1",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.0.0",
    "jest": "^29.7.0"
  }
